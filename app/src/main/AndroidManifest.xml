<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.micklarsen.rocketlaunch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.RocketLaunch">

        <!--        google maps key. if maps will stop working for some reason you could create a new project in google api and change the api key-->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyD2vRZsQi5wQOVTfbBAEkQ3GWLuWjFJkiQ" />

        <activity android:name=".DetailsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>